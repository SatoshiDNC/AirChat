        }
        if (e.button == 2) {
          for (const h of p.hitList.hits) {
            const g = h.gad;
            if (!g.ownedBy && !!(g.actionFlags & vp.GAF_CONTEXTMENU)) {
              g.gestureState = 'begin-menu'; g.ownedBy = index;
